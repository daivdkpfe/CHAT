global.SendEmail=function(to,subject,contect,callback)
{
    post('api.sendcloud.net', '/apiv2/mail/send', {
        apiUser:config.email_apiUser,
        apiKey:config.email_apiKey,
        from: config.email_from,
        to: to,
        subject: subject,
        fromName:config.email_fromName,
        html: contect
    }, function (result) {
        callback(result);
    });
}



