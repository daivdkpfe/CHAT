global.ad_unzip=function(strs,callback){
    const buffer = Buffer.from(strs, 'base64');
    
    zlib.unzip(buffer, (err, buffer) => {
        if (!err) {
        callback(unserialize(buffer.toString()));
    } else {
      console.log("erring");
        // handle error
    }
})
};
